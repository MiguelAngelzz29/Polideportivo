<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:targetApi="31">
        <activity
            android:name=".activity.MisReservasActivity"
            android:exported="false" />
        <activity
            android:name=".activity.CentroActivity"
            android:exported="false" />
        <activity
            android:name=".activity.MisReservasActividadesActivity"
            android:exported="false" />
        <activity
            android:name=".activity.SinReservasActivity"
            android:exported="false" />
        <activity
            android:name=".activity.SeleccionarActividadActivity"
            android:exported="false" />
        <activity
            android:name=".activity.ReservaActivity"
            android:exported="false" />
        <activity
            android:name=".activity.ConfirmarReservaActivity"
            android:exported="false" />
        <activity
            android:name=".activity.HorarioActivity"
            android:exported="false"
            android:parentActivityName=".activity.MainActivity" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.RegistroActivity"
            android:exported="false"
            android:parentActivityName=".activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.MainActivity" />
        </activity>
        <activity
            android:name=".activity.InicioActivity"
            android:exported="false"
            android:parentActivityName=".activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.MainActivity" />
        </activity>
    </application>

</manifest>